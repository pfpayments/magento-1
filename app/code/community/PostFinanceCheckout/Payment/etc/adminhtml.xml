<config>
	<menu>
	</menu>
	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<postfinancecheckout_payment translate="title">
										<title>PostFinance Checkout Payment</title>
										<sort_order>100</sort_order>
									</postfinancecheckout_payment>
								</children>
							</config>
						</children>
					</system>
					<customer>
						<children>
							<manage>
								<children>
									<postfinancecheckout_token translate="title">
		                                <title>Manage PostFinance Checkout Tokens</title>
		                                <sort_order>100</sort_order>
		                            </postfinancecheckout_token>
								</children>
							</manage>
						</children>
					</customer>
				</children>
			</admin>
		</resources>
	</acl>
</config>